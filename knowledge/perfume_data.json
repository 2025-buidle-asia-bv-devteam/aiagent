    "manufacturing": {
      "eau_de_parfum": {
        "fragrance_oil": "15-20%",
        "ethanol": "70-80%",
        "distilled_water": "5-10%",
        "process": [
          "Mix fragrance oils in ethanol slowly, stirring gently to blend.",
          "Add distilled water gradually to avoid separation.",
          "Age for 2-4 weeks in a cool (15-20°C), dark place.",
          "Filter through a coffee filter to remove sediment.",
          "Bottle in sterilized glass containers."
        ],
        "tips": {
          "high_volatility": "For top-heavy blends, use 68% ethanol and age for 3 weeks.",
          "woody_base": "For woody bases, increase fragrance oil to 22% and age for 4 weeks.",
          "floral": "Add 2% rose water for floral blends to enhance softness."
        }
      },
      "eau_de_toilette": {
        "fragrance_oil": "5-15%",
        "ethanol": "80-90%",
        "distilled_water": "5-10%",
        "process": [
          "Mix oils in ethanol quickly for lighter diffusion.",
          "Add water and stir gently.",
          "Age for 1-2 weeks.",
          "Filter and bottle."
        ],
        "tips": {
          "citrus": "Use 85% ethanol for citrus-heavy blends to maintain freshness."
        }
      },
      "creative_adjustments": "For experimental notes, test small batches (10ml). Adjust ethanol based on note intensity (e.g., smoky notes may need 65% ethanol for depth)."
    },
